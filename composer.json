{
    "name": "bit-mx/data-entities",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BitMx\\DataEntities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitMx\\DataEntities\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Jorge Enr√≠quez",
            "email": "44071711+jenriquez-bit@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0",
        "illuminate/database": "^10.0 || ^11.0",
        "fakerphp/faker": "^1.23"
    },
    "require-dev": {
        "laravel/pint": "^1.15",
        "phpstan/extension-installer": "^1.3",
        "larastan/larastan": "^2.9",
        "tomasvotruba/type-coverage": "^0.2.5",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BitMx\\DataEntities\\DataEntitiesServiceProvider"
            ]
        }
    }
}
