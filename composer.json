{
    "name": "bit-mx/data-entities",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BitMx\\DataEntities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitMx\\DataEntities\\Tests\\": "tests",
            "Utils\\Rector\\": "utils/rector/src",
            "Utils\\Rector\\Tests\\": "utils/rector/tests"
        }
    },
    "authors": [
        {
            "name": "Jorge Enr√≠quez",
            "email": "44071711+jenriquez-bit@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "fakerphp/faker": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.15",
        "phpstan/extension-installer": "^1.3",
        "larastan/larastan": "^3.0",
        "tomasvotruba/type-coverage": "^2.0",
        "orchestra/testbench": "^8.0|^9.0|^v10.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.3",
        "symfony/var-dumper": "^7.1",
        "rector/rector": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BitMx\\DataEntities\\DataEntitiesServiceProvider"
            ]
        }
    }
}
